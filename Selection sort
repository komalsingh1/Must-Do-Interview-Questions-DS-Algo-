#include<bits/stdc++.h>
 using namespace std;
 int main()
{
    int i,j,n,loc,temp,min;
   
    cin>>n;//no of elements in an array
   
    int a[n];
    for(i=0;i<n;i++)
    cin>>a[i];
    for(i=0;i<n-1;i++)
    {
        min=a[i];
        loc=i;
        for(j=i+1;j<n;j++)
        {
            if(min>a[j])
            {
                min=a[j];
                loc=j;
            }
        }
 
        temp=a[i];
        a[i]=a[loc];
        a[loc]=temp;
    }
    for(i=0;i<n;i++)
        cout<<a[i]<<" ";
    return 0;
}
